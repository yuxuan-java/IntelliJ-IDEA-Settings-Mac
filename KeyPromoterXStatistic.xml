<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close All">
        <Item>
          <IdeaActionID>CloseAllEditors</IdeaActionID>
          <ShortCut>'⌃⌥W'</ShortCut>
          <Description>Close All</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit Directory...">
        <Item>
          <IdeaActionID>CheckinFiles</IdeaActionID>
          <ShortCut>'⌃⇧J'</ShortCut>
          <Description>Commit Directory...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'EurekaServerApplication'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌥⇧D'</ShortCut>
          <Description>Debug 'EurekaServerApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Event Log">
        <Item>
          <IdeaActionID>ActivateEventLogToolWindow</IdeaActionID>
          <ShortCut>'⇧⌘E'</ShortCut>
          <Description>Event Log</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⌃⇧F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Implementation(s)">
        <Item>
          <IdeaActionID>GotoImplementation</IdeaActionID>
          <ShortCut>'⌥T'</ShortCut>
          <Description>Implementation(s)</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⌃⇧F12'</ShortCut>
          <Description>Restore Windows</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'⌥4'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'EurekaServerApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌃F2'</ShortCut>
          <Description>Stop 'EurekaServerApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>