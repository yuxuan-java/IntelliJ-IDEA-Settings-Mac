<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close All">
        <Item>
          <IdeaActionID>CloseAllEditors</IdeaActionID>
          <ShortCut>'⌃⌥W'</ShortCut>
          <Description>Close All</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit Directory...">
        <Item>
          <IdeaActionID>CheckinFiles</IdeaActionID>
          <ShortCut>'⌃⇧J'</ShortCut>
          <Description>Commit Directory...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌃C' or '⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Data Source Properties">
        <Item>
          <IdeaActionID>DatabaseView.PropertiesAction</IdeaActionID>
          <ShortCut>'⇧⏎'</ShortCut>
          <Description>Data Source Properties</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'EurekaServerApplication'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌥⇧D'</ShortCut>
          <Description>Debug 'EurekaServerApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'XiaoTaoApplication'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌥⇧D'</ShortCut>
          <Description>Debug 'XiaoTaoApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Disconnect">
        <Item>
          <IdeaActionID>DatabaseView.CloseConnectionAction</IdeaActionID>
          <ShortCut>'⌃F2'</ShortCut>
          <Description>Disconnect</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Event Log">
        <Item>
          <IdeaActionID>ActivateEventLogToolWindow</IdeaActionID>
          <ShortCut>'⇧⌘E'</ShortCut>
          <Description>Event Log</Description>
          <Count>23</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⌃⇧F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Maven">
        <Item>
          <IdeaActionID>ActivateMavenToolWindow</IdeaActionID>
          <ShortCut>'⌥M'</ShortCut>
          <Description>Maven</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="MyBatis Log Plugin">
        <Item>
          <IdeaActionID>TailMyBatisLog</IdeaActionID>
          <ShortCut>'⌃⌥⇧O'</ShortCut>
          <Description>MyBatis Log Plugin</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌥1'</ShortCut>
          <Description>Project</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Push...">
        <Item>
          <IdeaActionID>Vcs.Push</IdeaActionID>
          <ShortCut>'⌃⇧K'</ShortCut>
          <Description>Push...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⌃⇧F12'</ShortCut>
          <Description>Restore Windows</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'⌥4'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Scan keys">
        <Item>
          <IdeaActionID>ScanKeysAction</IdeaActionID>
          <ShortCut>'⌃⏎'</ShortCut>
          <Description>Scan keys</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'EurekaServerApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌃F2'</ShortCut>
          <Description>Stop 'EurekaServerApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Database">
        <Item>
          <IdeaActionID>ActivateDatabaseToolWindow</IdeaActionID>
          <Description>Database</Description>
        </Item>
      </Statistic>
      <Statistic Action="Hide ignored files">
        <Item>
          <IdeaActionID>HideIgnoredFiles</IdeaActionID>
          <Description>Hide ignored files</Description>
        </Item>
      </Statistic>
      <Statistic Action="Implementation(s)">
        <Item>
          <IdeaActionID>GotoImplementation</IdeaActionID>
          <ShortCut>'⌥T'</ShortCut>
          <Description>Implementation(s)</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>